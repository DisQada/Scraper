{
  "displayName": "Scraper",
  "name": "@disqada/scraper",
  "version": "1.0.0",
  "description": "Very simple and easy to use web scraper",
  "scripts": {
    "download": "node ./bin/download.js",
    "types": "workspace ./workspace.json -t",
    "docs": "workspace ./workspace.json -t -d",
    "format": "prettier . --write --ignore-path .gitignore",
    "test": "mocha ./test/**/*.test.js"
  },
  "bin": "./bin/download.js",
  "type": "module",
  "main": "./src/exports.js",
  "types": "./types/exports.d.ts",
  "exports": {
    ".": {
      "import": "./src/exports.js",
      "default": "./src/exports.js",
      "types": "./types/exports.d.ts"
    }
  },
  "files": [
    "bin",
    "src",
    "types",
    "LICENSE",
    "*.md"
  ],
  "homepage": "https://disqada.github.io/Scraper",
  "repository": {
    "type": "git",
    "url": "https://github.com/DisQada/Scraper.git"
  },
  "bugs": {
    "url": "https://github.com/DisQada/Scraper/issues"
  },
  "keywords": [
    "web",
    "scrape",
    "scraper",
    "selector",
    "search",
    "web scraper"
  ],
  "author": "DisQada <hi@disqada.org> (https://links.disqada.org)",
  "contributors": [],
  "license": "Apache-2.0",
  "devDependencies": {
    "@disqada/workspace": "^1.5.2",
    "mocha": "^10.7.3",
    "prettier": "^3.3.3"
  }
}
